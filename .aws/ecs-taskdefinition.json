{
    "family": "avcorn-taskdef",
    "containerDefinitions": [
        {
            "Name": "avcorn-container",
            "Essential": "true",
            "Image": "avcorn-image",
            "MemoryReservation": 400,
            "MountPoints": [
                {
                    "ContainerPath": "/var/www",
                    "SourceVolume": "avcorn-volume"
                }
            ],
            "PortMappings": [
                {
                    "ContainerPort": 80
                }
            ]
        }
    ],
    "volumes": [
        {
            "name": "avcorn-volume",
            "host": {}
        }
    ],
    "placementConstraints": [],
    "tags": []
}