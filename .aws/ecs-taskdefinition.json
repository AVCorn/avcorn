{
    "family": "avcorn-taskdef",
    "containerDefinitions": [
        {
            "Name": "avcorn-container",
            "Essential": "true",
            "Image": "avcorn-image",
            "Cpu": 0,
            "MemoryReservation": 409,
            "MountPoints": [
                {
                    "ContainerPath": "/var/www",
                    "SourceVolume": "avcorn-volume"
                }
            ],
            "PortMappings": [
                {
                    "Name": "web",
                    "ContainerPort": 80,
                    "HostPort": 0,
                    "Protocol": "tcp",
                    "AppProtocol": "http"
                }
            ],
            "DnsServers": [
                "8.8.8.8"
            ]
        }
    ],
    "volumes": [
        {
            "name": "avcorn-volume",
            "host": {}
        }
    ],
    "placementConstraints": [],
    "tags": [],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}